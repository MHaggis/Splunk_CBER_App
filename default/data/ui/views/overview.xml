<form>
  <label>Overview</label>
  <fieldset submitButton="false">
    <input type="time" token="timeframe">
      <label></label>
      <default>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <row>
    <html>
        <h1>
          Carbon Black Enterprise Response
        </h1>
        <p>Overview of daily usage and analytics of your Carbon Black Enterprise Response environment</p>

      </html>
  </row>
  <row>
    <panel>
      <!-- 3 elements are grouped into the left column -->
      <single>
        <search>
          <query>| savedsearch "CBER - Alert Activity" | stats count</query>
          <earliest>$timeframe.earliest$</earliest>
          <latest>$timeframe.latest$</latest>
        </search>
        <option name="underLabel">sensors reporting alerts</option>
        <option name="field">count</option>
        <option name="linkFields">result, afterlabel</option>
        <option name="linkSearch">index=_internal earliest=-h | stats count</option>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="numberPrecision">0</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="useColors">0</option>
        <option name="useThousandSeparators">1</option>
        <option name="linkView">search</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0xd93f3c","0xf7bc38","0x65a637"]</option>
        <option name="rangeValues">[0,1000]</option>
        <option name="trendInterval">auto</option>
      </single>
      <single>
        <search>
          <query>| tstats count from datamodel=Carbon_Black.CB_Alerts</query>
          <earliest>$timeframe.earliest$</earliest>
          <latest>$timeframe.latest$</latest>
        </search>
        <option name="underLabel">alerts triggered</option>
        <option name="field">count</option>
        <option name="linkFields">result, afterlabel</option>
        <option name="linkSearch">| tstats count from datamodel=Carbon_Black.CB_Alerts by CB_Alerts.computer_name</option>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="numberPrecision">0</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
        <option name="linkView">search</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x65a637","0xf7bc38","0xf58f39","0xd93f3c"]</option>
        <option name="rangeValues">[0,50,150]</option>
        <option name="trendInterval">auto</option>
      </single>
      <single>
        <search>
          <query>| tstats count from datamodel=Carbon_Black.CB_Alerts where CB_Alerts.feed_name="cbbanning"</query>
          <earliest>$timeframe.earliest$</earliest>
          <latest>$timeframe.latest$</latest>
        </search>
        <option name="underLabel">banned hashes executed</option>
        <option name="field">count</option>
        <option name="linkFields">result, afterlabel</option>
        <option name="linkView">search</option>
        <option name="linkSearch">| tstats count from datamodel=Carbon_Black.CB_Alerts where CB_Alerts.feed_name="cbbanning" by CB_Alerts.computer_name, CB_Alerts.process_name</option>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0x65a637","0xf7bc38","0xf58f39","0xd93f3c"]</option>
        <option name="rangeValues">[5,15,30]</option>
        <option name="trendInterval">auto</option>
      </single>
      <single>
        <search>
          <query>| tstats count from datamodel=Carbon_Black.CB_Alerts by CB_Alerts.cb_server
| stats count(CB_Alerts.cb_server)</query>
          <earliest>$timeframe.earliest$</earliest>
          <latest>$timeframe.latest$</latest>
        </search>
        <option name="underLabel">master servers sending data</option>
        <option name="field">count</option>
        <option name="linkFields">result, afterlabel</option>
        <option name="linkView">charting</option>
        <option name="linkSearch">| tstats count from datamodel=Carbon_Black.CB_Alerts by CB_Alerts.cb_server</option>
        <option name="colorBy">value</option>
        <option name="colorMode">none</option>
        <option name="numberPrecision">0</option>
        <option name="showSparkline">1</option>
        <option name="showTrendIndicator">1</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="useColors">1</option>
        <option name="useThousandSeparators">1</option>
        <option name="drilldown">none</option>
        <option name="rangeColors">["0xd93f3c","0x65a637"]</option>
        <option name="rangeValues">[0]</option>
        <option name="trendInterval">auto</option>
      </single>
    </panel>
  </row>
  <row>
    <chart>
      <title>Watchlist Events</title>
      <search>
        <query>| tstats count from datamodel=Carbon_Black.CB_Alerts by CB_Alerts.watchlist_name | rename CB_Alerts.watchlist_name as alert | where count&gt;1</query>
        <earliest>$timeframe.earliest$</earliest>
        <latest>$timeframe.latest$</latest>
      </search>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
      <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisTitleY2.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.axisY2.enabled">0</option>
      <option name="charting.axisY2.scale">inherit</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.bubbleMaximumSize">50</option>
      <option name="charting.chart.bubbleMinimumSize">10</option>
      <option name="charting.chart.bubbleSizeBy">area</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.showDataLabels">none</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.axisY.maximumNumber">100</option>
    </chart>
    <chart>
      <title>Feed Events</title>
      <search>
        <query>| tstats count from datamodel=Carbon_Black.CB_Alerts by CB_Alerts.feed_name | rename CB_Alerts.feed_name as alert</query>
        <earliest>$timeframe.earliest$</earliest>
        <latest>$timeframe.latest$</latest>
      </search>
      <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
      <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisTitleY2.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.axisY2.enabled">0</option>
      <option name="charting.axisY2.scale">inherit</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.bubbleMaximumSize">50</option>
      <option name="charting.chart.bubbleMinimumSize">10</option>
      <option name="charting.chart.bubbleSizeBy">area</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.showDataLabels">none</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.layout.splitSeries.allowIndependentYRanges">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="charting.axisY.maximumNumber">100</option>
    </chart>
  </row>
</form>
